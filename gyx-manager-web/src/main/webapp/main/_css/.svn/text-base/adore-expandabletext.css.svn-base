.input-field {
    position: relative;
    margin-top: 1rem;
}

.mdi-action-search:before {
    content: "\e661";
}

.adore-expandabletext-button-search {
    color: initial;
    position: absolute;
    right: 5px;
    top: 0px;
    font-size: 23px;
    display: none;
}

.adore_dropDown {
    z-index: 1000;
    overflow: auto;
    -webkit-transform: translateY(-15px);
    transform: translateY(-15px);
    opacity: 1;
    transition: opacity 0.3s, -webkit-transform 0.3s cubic-bezier(0.25, 0.5, 0.5, 0.9);
    transition: opacity 0.3s, transform 0.3s cubic-bezier(0.25, 0.5, 0.5, 0.9);
    transition: opacity 0.3s, transform 0.3s cubic-bezier(0.25, 0.5, 0.5, 0.9), -webkit-transform 0.3s cubic-bezier(0.25, 0.5, 0.5, 0.9);
    position: absolute;
    margin-top: 3px;
    box-shadow: none;
    min-width: 135px;
    padding: 0;
}

.dropdown-content {
    background-color: #fff;
    margin: 0;
    display: none;
    min-width: 100px;
    max-height: 650px;
    overflow-y: auto;
    opacity: 0;
    position: absolute;
    z-index: 999;
    will-change: width, height;
    box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.16), 0 2px 10px 0 rgba(0, 0, 0, 0.12);
}

.adore-dropDown-control {
    position: absolute;
    display: block;
    height: 30px;
    background-color: white;
    border-bottom: 1px solid beige;
    z-index: 1000;
}

.adore-dropDown-control > .to-pre {
    position: absolute;
    display: block;
    left: 0;
    width: 25%;
    height: 100%;
    background-color: white;
    text-align: center;
}

.mdi-hardware-keyboard-arrow-left:before {
    content: "\f0d9";
}

.adore-dropDown-control > .to-next {
    position: absolute;
    display: block;
    right: 0;
    width: 25%;
    height: 100%;
    background-color: white;
    text-align: center;
}

.mdi-hardware-keyboard-arrow-right:before {
    content: "\f0da";
}

.adore-dropDown-control > .page-index {
    position: absolute;
    display: block;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 100px;
    height: 25px;
    color: black;
    font-size: 14px;
    line-height: 25px;
    text-align: center;
    background-color: white;
}

.dropdown-content {
    background-color: #fff;
    margin: 0;
    display: none;
    min-width: 100px;
    max-height: 650px;
    overflow-y: auto;
    opacity: 0;
    position: absolute;
    z-index: 999;
    will-change: width, height;
}

.dropdown-content li {
    clear: both;
    color: rgba(0, 0, 0, 0.87);
    cursor: pointer;
    max-height: 56px;
    line-height: 1.5rem;
    width: 100%;
    text-align: left;
    text-transform: none;
    list-style-type: none;
}

.dropdown-content li:hover, .dropdown-content li.active, .dropdown-content li.selected {
    background-color: #eee;
}

.dropdown-content li.active.selected {
    background-color: #e1e1e1;
}

.dropdown-content li.divider {
    min-height: 0;
    height: 1px;
}

.dropdown-content li > a, .dropdown-content li > span {
    font-size: 15px;
    color: #26a69a;
    display: block;
    line-height: 22px;
    padding: 2px 10px;
}

.dropdown-content li > span > label {
    top: 1px;
    left: 3px;
    height: 18px;
}

.dropdown-content li > a > i {
    height: inherit;
    line-height: inherit;
}

.adore-expandableText input[type=text] {
    background-color: rgba(251, 251, 251, 0.2);
    border: 0;
    border-radius: 3px;
    outline: none;
    font-size: 1rem;
    padding: 0;
    padding-left: 0.25rem;
    box-shadow: none;
    box-sizing: content-box;
    transition: all 0s;
    margin: 0;
    height: 37px;
}