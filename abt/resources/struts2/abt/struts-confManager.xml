		<!-- 主题配置与应用 -->
		<action name="themeManager_*" class="themeManagerAction" method="{1}">   
			<interceptor-ref name="SystemConsoleInterceptorStack"></interceptor-ref>
			<interceptor-ref name="fileUpload">   
			<!--拦截器strut2自带的, 指定上传文件的格式,如果不符合规定，将会自动拦截下来 -->  
		        <param name="allowedTypes">image/bmp,image/png,image/gif,image/jpeg,image/jpg</param>
		        <param name="maximumSize">20000000000</param>
		    </interceptor-ref>  
		    <interceptor-ref name="defaultStack" />
		    <result name="success" >/webpages/abt/configManager/themeManager.jsp</result>
		    <result name="success_manager" >/webpages/abt/configManager/themeConfig.jsp</result>
		</action>
		
