package com.agree.abt.model.devmanager;

import java.io.Serializable;

/**
 * 自助设备附属参数实体类
 */
public class DeviceInfo4SelfService implements Serializable{
	private static final long serialVersionUID = 1L;
	/**
	 * 在离行 
	 * 1-在 2-离
	 */
	private String laying_mode;
	/**
	 * 购置日期
	 */
	private String buydate;
	/**
	 * 安装日期
	 */
	private String installdate;
	/**
	 * 发卡机管理员列表
	 */
	private String card_admin;
	/**
	 * 审核员列表
	 */
	private String device_assessor;
	/**
	 * 装机地址
	 */
	private String device_address;
	
	public String getLaying_mode() {
		return laying_mode;
	}
	public void setLaying_mode(String laying_mode) {
		this.laying_mode = laying_mode;
	}
	public String getBuydate() {
		return buydate;
	}
	public void setBuydate(String buydate) {
		this.buydate = buydate;
	}
	public String getInstalldate() {
		return installdate;
	}
	public void setInstalldate(String installdate) {
		this.installdate = installdate;
	}
	public String getCard_admin() {
		return card_admin;
	}
	public void setCard_admin(String card_admin) {
		this.card_admin = card_admin;
	}
	public String getDevice_assessor() {
		return device_assessor;
	}
	public void setDevice_assessor(String device_assessor) {
		this.device_assessor = device_assessor;
	}
	public String getDevice_address() {
		return device_address;
	}
	public void setDevice_address(String device_address) {
		this.device_address = device_address;
	}
	
	public DeviceInfo4SelfService(String laying_mode, String buydate, String installdate,
			String card_admin, String device_assessor, String device_address) {
		super();
		this.laying_mode = laying_mode;
		this.buydate = buydate;
		this.installdate = installdate;
		this.card_admin = card_admin;
		this.device_assessor = device_assessor;
		this.device_address = device_address;
	}
	
	public DeviceInfo4SelfService() {
		super();
	}
}
