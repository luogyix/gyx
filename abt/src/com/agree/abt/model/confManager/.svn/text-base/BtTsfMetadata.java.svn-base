package com.agree.abt.model.confManager;

import java.io.Serializable;

/**
 * 自助元数据表
 * @author hujuqiang
 * @date 2016-9-1
 */
@SuppressWarnings("serial")
public class BtTsfMetadata implements Serializable{
	private String metadata_id;//元数据id PK
	private String menu_code;//菜单码
	private String menu_name;//菜单名称
	private String trade_flag;//菜单标志,0-管理菜单 1-业务菜单
	private String menu_description;//菜单描述
	private String menu_role;//菜单所属角色 1-审核岗,2-管理岗,3-维护岗
	private String menutype;//是否授权,0-直接使用,1-需授权使用
	private String menustatus;//菜单状态,0-可见 1-隐藏
	private String deleteflag;//删除标志,0-正常菜单,1-已弃用菜单
	private String busitype;//所属业务类别,0-对公 1-个人
	private String menu_serv_time;//菜单生效时间段
	private String disablestatus;//正常菜单未生效时状态,0-不可见、1-加锁、2-置灰
	private String interface_id;//界面id
	private String device_type;//所属设备
	public String getDevice_type() {
		return device_type;
	}
	public void setDevice_type(String device_type) {
		this.device_type = device_type;
	}
	private BtSysInterface sysInterface;
	private String node_location;
	public String getNode_location() {
		return node_location;
	}
	public void setNode_location(String node_location) {
		this.node_location = node_location;
	}
	public BtSysInterface getSysInterface() {
		return sysInterface;
	}
	public void setSysInterface(BtSysInterface sysInterface) {
		this.sysInterface = sysInterface;
	}
	public String getInterface_id() {
		return interface_id;
	}
	public void setInterface_id(String interface_id) {
		this.interface_id = interface_id;
	}
	public String getMenu_code() {
		return menu_code;
	}
	public void setMenu_code(String menu_code) {
		this.menu_code = menu_code;
	}
	public String getMetadata_id() {
		return metadata_id;
	}
	public void setMetadata_id(String metadata_id) {
		this.metadata_id = metadata_id;
	}
	public String getMenu_name() {
		return menu_name;
	}
	public void setMenu_name(String menu_name) {
		this.menu_name = menu_name;
	}
	public String getTrade_flag() {
		return trade_flag;
	}
	public void setTrade_flag(String trade_flag) {
		this.trade_flag = trade_flag;
	}
	public String getMenu_description() {
		return menu_description;
	}
	public void setMenu_description(String menu_description) {
		this.menu_description = menu_description;
	}
	public String getMenu_role() {
		return menu_role;
	}
	public void setMenu_role(String menu_role) {
		this.menu_role = menu_role;
	}
	public String getMenutype() {
		return menutype;
	}
	public void setMenutype(String menutype) {
		this.menutype = menutype;
	}
	public String getMenustatus() {
		return menustatus;
	}
	public void setMenustatus(String menustatus) {
		this.menustatus = menustatus;
	}
	public String getDeleteflag() {
		return deleteflag;
	}
	public void setDeleteflag(String deleteflag) {
		this.deleteflag = deleteflag;
	}
	public String getBusitype() {
		return busitype;
	}
	public void setBusitype(String busitype) {
		this.busitype = busitype;
	}
	public String getMenu_serv_time() {
		return menu_serv_time;
	}
	public void setMenu_serv_time(String menu_serv_time) {
		this.menu_serv_time = menu_serv_time;
	}
	public String getDisablestatus() {
		return disablestatus;
	}
	public void setDisablestatus(String disablestatus) {
		this.disablestatus = disablestatus;
	}
}